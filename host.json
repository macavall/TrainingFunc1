{
    "version": "2.0",
    "logging": {
        "fileLoggingMode": "always",
        "console": {
            "isEnabled": true
        },
        "logLevel": {
          "default": "Trace",
          "Microsoft": "Trace",
          "Host.Aggregator": "Trace",
          "Host.Results": "Trace",
          "Worker": "Trace"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}